{
    "name": "peterbenke/pb-pdf",
    "type": "typo3-cms-extension",
    "description": "Pdf generation for various usages",
    "keywords": ["TYPO3", "extension", "pdf"],
    "homepage": "http://www.typomotor.de",
    "authors": [
        {
            "name": "Peter Benke",
            "role": "Developer",
            "homepage": "http://www.typomotor.de"
        }
    ],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": "^11.5",
        "dompdf/dompdf": "2.0.1"
    },
    "autoload": {
        "psr-4": {
            "PeterBenke\\Pdf\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "pb_pdf"
        }
    }
}
